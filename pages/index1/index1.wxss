.container{
  position: relative;
  z-index: 1;
}
.sideBar{
  height: 100%;
  width: 750rpx;
  position: fixed;
  background-color: #ddd;
  z-index: 0;
}
.open .sideBar{
  
}
.wrap{
  height: 100%;
  position: fixed;
  width: 750rpx;
  background-color: #fff;
  z-index: 0;
  transition: all 0.4s ease; 
}

.appBar{
  width: 100%;
  height: 80rpx;
  background-color: #ccc;
  text-align: center;
  line-height: 80rpx;
  position: sticky;
  top: 0;
  z-index: 10;
}
.tabPage{
  position: relative;
}
.tabPage .item{
  position: absolute;
  width: 100%;
  top: 0;
  transform: translate(0%, 0px);

}